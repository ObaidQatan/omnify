    "next": "12.3.1",
    "next-translate": "^1.5.0",
    "nextjs-progressbar": "^0.0.14",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-simple-image-slider": "^2.4.1",
    "react-spinners": "^0.13.5",
    "react-tooltip": "^4.4.3",
