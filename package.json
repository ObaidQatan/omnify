    "@mantine/dropzone": "^5.1.5",
    "@mantine/form": "^5.1.5",
    "@mantine/hooks": "^5.1.5",
    "@mantine/modals": "^5.1.5",
    "@mantine/next": "^5.1.5",
    "@mantine/notifications": "^5.1.5",
    "@mantine/nprogress": "^5.1.5",
    "@mantine/prism": "^5.1.5",
    "@prisma/client": "^4.4.0",
    "@supabase/supabase-js": "^2.0.5",
    "bcrypt": "^5.0.1",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.1",
    "framer-motion": "^7.6.18",
