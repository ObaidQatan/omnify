{
  "name": "omnify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "prisma migrate dev --schema=./db/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=./db/prisma/schema.prisma",
    "postinstall": "prisma generate --schema=./db/prisma/schema.prisma",
    "db:push": "prisma db push --schema=./db/prisma/schema.prisma",
    "db:seed": "ts-node db/seed.ts",
    "db:studio": "prisma studio --schema=./db/prisma/schema.prisma",
    "dev": "concurrently \"npm:start:dev\" \"npm:db:generate\""
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
